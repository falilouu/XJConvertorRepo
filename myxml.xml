<EABooks Name="BooksContainer" >

  <entity Name="Livre">
    <attribute entity="Livre" Name="idLivre"/>
  </entity>
  <entity Name="Editeur">
    <attribute entity="Editeur" Name="idEditeur"/>
  </entity>
  <entity Name="Auteur">
    <attribute entity="Auteur" Name="idAuteur"/>
  </entity>

  <association entity1="Editeur" entity2="Livre">Publier</association>
  <association entity1="Auteur" entity2="Livre">Ecrire</association>

</EABooks>  